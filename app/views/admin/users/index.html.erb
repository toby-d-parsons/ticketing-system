<h1>Users</h1>

<div>
    <%= link_to "New", new_admin_user_path %>
</div>

<table>
    <tr>
        <th>ID</th>
        <th>Email Address</th>
        <th>Role</th>
        <th></th>
    </tr>
    <% @users.each do |user| %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.email_address %></td>
            <td><%= user.role.name %></td>
            <td><%= link_to "Edit", edit_admin_user_path(user.role_id) %>
        </tr>
    <% end %>
</table>