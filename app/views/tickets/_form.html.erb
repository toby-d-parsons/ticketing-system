<%= form_with model: ticket do |form| %>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title %>
    </div>
    <div>
        <%= form.label :description %>
        <%= form.text_field :description %>
    </div>
    <div>
        <%= form.label :status %>
        <%= form.text_field :status %>
    </div>

    <div>
        <%= form.submit %>
    </div>
<% end %>